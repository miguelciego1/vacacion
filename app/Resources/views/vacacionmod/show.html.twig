{% extends 'base.html.twig' %}

{% block page_content %}
    <h1>Vacacionmod</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vacacionMod.id }}</td>
            </tr>
            <tr>
                <th>Creadoel</th>
                <td>{% if vacacionMod.creadoEl %}{{ vacacionMod.creadoEl|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Motivo</th>
                <td>{{ vacacionMod.motivo }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ vacacionMod.tipo }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vacacionmod_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('vacacionmod_edit', { 'id': vacacionMod.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
